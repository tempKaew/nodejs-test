{
  "version": 2,
  "builds": [
    {
      "src": "./src/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/assets/img/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/app.js"
    },
    {
      "src": "/assets/img/.*",
      "dest": "/src/assets/img/$1"
    }
  ]
}